          todos: []\n        }\n    },\n    methods: {\n        retrieveTodos() {\n            TodoDataService.getAll()\n                .then(response => {\n                    this.todos = response.data\n                })\n                .catch(e => {\n                    alert(e)\n                })\n        }\n    },\n    mounted() {\n        this.retrieveTodos()\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}